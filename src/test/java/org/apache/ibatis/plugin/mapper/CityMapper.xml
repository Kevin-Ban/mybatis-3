<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.ibatis.plugin.mapper.CityMapper">
    <select id="queryByXml" parameterType="hashmap" resultType="hashmap">
        select * from city
    </select>
  <insert id="insertCity" keyProperty="id" useGeneratedKeys="true" parameterType="org.apache.ibatis.plugin.mapper.City">
    insert into city(code, name) values(#{code}, #{name})
  </insert>
</mapper>
